package TASession;
import java.util.*;
public class prices {
	public static void main(String[] args) {
	    Scanner sc=new Scanner(System.in);
	    Medicine[] med=new Medicine[4];
	    for(int i=0;i<med.length;i++)
	    {System.out.println("mane");
	    	String mname=sc.nextLine();
	    	System.out.println("batch");
	    	String batch=sc.nextLine();
	    	System.out.println("disease");
	    	String disease=sc.nextLine();
	    	System.out.println("pricevino");
	    	int price=sc.nextInt();
	    	sc.nextLine();
	        med[i]=new Medicine(mname,batch,disease,price);
	        System.out.println("inserted");
	    }
	    String search=sc.nextLine();
	    perform(search,med);
}

private static void perform(String search, Medicine[] med) {
		// TODO Auto-generated method stub
		for(int i=0;i<med.length;i++)
		{
			Medicine m=med[i];
			String r=m.disease;
			if(r.equals(search))
			{
				System.out.println(m.price);
			}
		}
			
		
	}
}

class Medicine
{
    String medicinename;
    String batchno;
    String disease;
    int price;
    Medicine(String medicinename,String batchno,String disease,int price)
    {
        this.medicinename=medicinename;
        this.batchno=batchno;
        this.disease=disease;
        this.price=price;
    }
}
